    "babel-preset-stage-2": "^6.11.0",
    "babel-register": "^6.11.6",
    "eslint": "^4.0.0",
    "mocha": "^3.0.1",
    "nodemon": "^1.11.0"
  },
  "dependencies": {
    "@google/maps": "^0.4.0",
    "axios": "^0.16.2",
    "cors": "^2.8.3",
    "express": "^4.15.3"
